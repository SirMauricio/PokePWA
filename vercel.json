{
    "version": 2,
    "builds": [
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/service-worker.js",
            "dest": "/service-worker.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}